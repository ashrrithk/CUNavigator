<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/teacher_dashboard"
    android:layout_height="match_parent"
    tools:context=".TeacherDashboard">

    <FrameLayout
        android:id="@+id/main_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/teacher_nav"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">


    </FrameLayout>


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/teacher_nav"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        app:menu="@menu/teacherdashboard_menu"
        app:itemBackground="@color/colorPrimaryDark"
        app:itemIconTint="@color/teacher_dashboard"
        app:itemTextColor="@color/teacher_dashboard"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        />

</RelativeLayout>